<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.learnenglishwordssecondtry">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ViewChangeWordActivity"
            android:parentActivityName=".ViewWordsActivity"/>
        <activity android:name=".ViewWordsActivity"
            android:parentActivityName=".ChooseSetToViewActivity"/>
        <activity android:name=".ChooseSetToViewActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ChooseSetToLearnActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".LearnWordsActivity"
            android:parentActivityName=".ChooseSetToLearnActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>